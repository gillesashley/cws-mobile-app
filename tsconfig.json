{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "paths": {
      "@/components/*": ["./components/*" ],
      "@/scripts/*": ["./scripts/*" ],
      "@/services/*": ["./services/*" ],
      "@/hooks/*": ["./hooks/*" ],
      "@/*": ["./*" ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
