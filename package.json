{
  "name": "cws-mobile-app",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:tunnel": "expo start --android --tunnel -c",
    "ios": "expo run:ios",
    "web": "expo start --web -c",
    "test": "jest --watchAll",
    "build:ios": "time eas build -e preview -p ios ${stage:-'--local'}",
    "build:ios:remote": "stage=\" \" npm run build:ios-d",
    "build:android": "time eas build -e preview -p android ${stage:-'--local'}",
    "build:android:preview": "eas build -p android -e preview --local",
    "build:android:remote": "stage=\" \" npm run build:android",
    "postinstall": "patch-package",
    "doctor": "yes | time npx expo install --check && npx expo-doctor",
    "log:adb": "adb logcat",
    "install:android:local": "adb install $(ls -t | grep build | head -n 1)",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "npx eslint --fix app/* components/* hooks/* services/*",
    "lint:prettier": "npx prettier -w app/* components/* hooks/* services/*"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.3",
    "expo": "~51.0.34",
    "expo-asset": "~10.0.10",
    "expo-blur": "~13.0.2",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-font": "~12.0.10",
    "expo-image-picker": "^15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.20",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.25",
    "expo-web-browser": "~13.0.3",
    "jest-expo": "~51.0.4",
    "jsonwebtoken": "^9.0.2",
    "nativewind": "^4.1.6",
    "patch-package": "^8.0.0",
    "postcss-loader": "^4.2.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-native": "0.74.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-error-boundary": "^1.2.4",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.12.0",
    "@putout/plugin-merge-duplicate-imports": "^11.0.0",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-path-alias": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "postcss": "^8.4.45",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-sort-imports": "^1.8.6",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.3.3"
  },
  "private": true,
  "resolutions": {
    "inflight": "2.0.0"
  }
}
