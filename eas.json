{
  "cli": {
    "version": ">= 10.2.1"
  },
  "build": {
    "development": {
      "env":{
        "EXPO_PUBLIC_API_URL":"https://campaignwithus.com/api"
      },
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env":{
        "EXPO_PUBLIC_API_URL":"https://campaignwithus.com/api"
      },
      "ios":{
        "simulator":true
      }
    },
    "production": {
      "env":{
        "EXPO_PUBLIC_API_URL":"https://campaignwithus.com/api"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
